                    //$('*').tooltip({ track: true });
                    $('[data-bs-toggle="tooltip"]').on('mouseleave', function () {
                        var $this = $(this);
                        $this.attr('title', $this.attr('title-cache'));
                        $this.attr('title-cache', '');
                        $('[data-bs-toggle="tooltip"], .tooltip').tooltip("hide");
                    });
                });
